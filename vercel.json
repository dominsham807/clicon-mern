{
    "version": 2,
    "builds": [
      {
        "src": "./clicon-server/server.js",   
        "use": "@vercel/node"
      },
      {
        "src": "./clicon-website/build",     
        "use": "@vercel/static"
      }
    ], 
  
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/clicon-server/index.js"
      }
    ]
  }