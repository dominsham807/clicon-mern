{
    "version": 2,
    "builds": [
        { 
            "src": "./index.js",
            "use": "@vercel/node" 
        },
        {
            "src": "./*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        },
        {
            "src": "/images/(.*)",
            "dest": "/"
        }
    ] 
}