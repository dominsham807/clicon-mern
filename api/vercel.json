{
    "version": 2,
    "builds": [
        { 
            "src": "./index.js",
            "use": "@vercel/node" 
        },
        {
            "src": "/images/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        { "handle": "filesystem" },
        {
            "src": "/(.*)",
            "dest": "/"
        },
        {
            "src": "/(.+)",
            "dest": "images/$1"
        }
    ] 
}